#: pg_ctl.c:2048
#, c-format
msgid ""
"\n"
"Options for register and unregister:\n"
msgstr ""
"\n"
"Opzioni per register e unregister:\n"

#: pg_ctl.c:2049
#, c-format
msgid ""
"  -N SERVICENAME  service name with which to register PostgreSQL server\n"
msgstr ""
"  -N SERVIZIO     nome del servizio con cui registrare il server PostgreSQL\n"

#: pg_ctl.c:2050
#, c-format
msgid "  -P PASSWORD     password of account to register PostgreSQL server\n"
msgstr ""
"  -P PASSWORD     password per l'account con cui registrare il server "
"PostgreSQL\n"

#: pg_ctl.c:2051
#, c-format
msgid "  -U USERNAME     user name of account to register PostgreSQL server\n"
msgstr ""
"  -U UTENTE       nome utente dell'account con cui registrare il server "
"PostgreSQL\n"

#: pg_ctl.c:2052
#, c-format
msgid "  -S START-TYPE   service start type to register PostgreSQL server\n"
msgstr ""
"  -S TIPO-AVVIO   tipo di avvio del servizio con cui registrare il server "
"PostgreSQL\n"

#: pg_ctl.c:2054
#, c-format
msgid ""
"\n"
"Start types are:\n"
msgstr ""
"\n"
"I tipi di avvio sono:\n"

#: pg_ctl.c:2055
#, c-format
msgid ""
"  auto       start service automatically during system startup (default)\n"
msgstr ""
"  auto       avvia il servizio automaticamente durante l'avvio del sistema "
"(predefinito)\n"

#: pg_ctl.c:2056
#, c-format
msgid "  demand     start service on demand\n"
msgstr "  demand     avvia il servizio quando richiesto\n"

